dilekçe = """
Tıp Fakğltesi Dekanlığına İstanbul {}/{}/{}
Konu: {}
Gereğinin yapılmasını arz ederim.Saygılarımla
Ad-Soyad: {}
Adres Bilgileri: {}
Telefon: {}
"""
gün = input("günü giriniz:")
ay = input("ayı giriniz:")
yıl = input("yılı giriniz:")
konu = input("konuyu yazınız:")
ad_soyad = input("ad soyad giriniz:")
adres = input("adresinizi giriniz:")
telefon = input("telefon giriniz:")

print(dilekçe.format(gün,ay, yıl, konu,ad_soyad,adres,telefon))
